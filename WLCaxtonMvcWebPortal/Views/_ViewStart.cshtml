    var actionName = Convert.ToString(HttpContext.Current.Request.RequestContext.RouteData.Values["action"]);
    string _ChkUser = string.Empty;
    if (controlerName == "Login" && actionName != "UserLogin")
    {

        if (Convert.ToString(WLCaxtonMvcWebPortal.Util.SessionClass.loggedUser.LoginDetails) != "")
